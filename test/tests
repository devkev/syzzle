check envvar_pattern 'LD_LIBRARY_PATH' "$basedir"/sw/def/1.1/lib
check envvar_pattern 'LD_RUN_PATH' "$basedir"/sw/def/1.1/lib

check modules_loaded 'abc/1.0'
